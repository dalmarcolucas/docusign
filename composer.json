{
    "name": "dalmarcolucas/docusign",
    "description": "DocuSign Rest API Wrapper for Laravel.",
    "keywords": [
        "laravel",
        "docusign",
        "laravel 8",
        "sign",
        "documents",
        "envelope",
        "contracts"
    ],
    "homepage": "https://github.com/dalmarcolucas/docusign",
    "license": "MIT",
    "authors": [
        {
            "name": "TJ Phippen",
            "email": "tj@tjphippen.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0"
    },
    "suggest": {
        "docusign/esign-client": "Official Docusign Client, For using JWT Authentication Methods",
        "league/oauth2-client": "For oAuth2 Support",
        "firebase/php-jwt": "For JWT Support"
    },
    "require-dev": {
       "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "Tjphippen\\Docusign\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dalmarcolucas/docusign"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.4-dev"
        },
        "laravel": {
            "providers": [
                "Tjphippen\\Docusign\\DocusignServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
